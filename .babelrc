{
    "presets": [
      ["env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }],
      "stage-2"
    ],
    "plugins": ["transform-runtime",["component", [
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ]]],
    "env": {
      "test": {
        "presets": ["env", "es2015", "stage-4"],
        "plugins": ["istanbul"]
      }
    }
  }
